alias ..='cd ..'
alias ...='cd ../..'
alias ll='ls -lhBG'
alias la='ls -lha'
alias l='ls -CF'
alias h='history'
alias s='screen -D -R'
alias pu='pushd'
alias po='popd'
alias alert='notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal || echo error)" "$(history|tail -n1|sed -e '\''s/^\s*[0-9]\+\s*//;s/[;&|]\s*alert$//'\'')"'
